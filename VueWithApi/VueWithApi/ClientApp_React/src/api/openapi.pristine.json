  "components": {
    "schemas": {
      "JToken": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/JToken"
        }
      },
      "PatchUserInputModel": {
        "type": "object",
        "additionalProperties": {
          "$ref": "#/components/schemas/JToken"
        }
      },
      "SetPasswordInputModel": {
        "required": [
          "newPassword"
        ],
        "type": "object",
        "properties": {
          "applicationId": {
            "type": "string",
            "nullable": true
          },
          "oldPassword": {
            "type": "string",
            "nullable": true
          },
          "newPassword": {
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "RemovePasswordInputModel": {
        "type": "object",
        "properties": {
          "applicationId": {
            "type": "string",
            "nullable": true
          },
          "oldPassword": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ChangeEmailInputModel": {
        "required": [
          "newEmail"
        ],
        "type": "object",
        "properties": {
          "applicationId": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          },
          "newEmail": {
            "pattern": ".+\\@.+\\..+",
            "type": "string",
            "format": "email"
          }
        },
        "additionalProperties": false
      },
      "RegisterInputModel": {
        "required": [
          "email"
        ],
        "type": "object",
        "properties": {
          "applicationId": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "pattern": ".+\\@.+\\..+",
            "type": "string",
            "format": "email"
          },
          "password": {
            "type": "string",
            "nullable": true
          },
          "claims": {
            "type": "object",
            "additionalProperties": {
              "type": "string"
            },
            "nullable": true
          },
          "setPassword": {
            "type": "boolean"
          },
          "nextUrl": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "SendCodeInputModel": {
        "required": [
          "username"
        ],
        "type": "object",
        "properties": {
          "applicationId": {
            "type": "string",
            "nullable": true
          },
          "username": {
            "type": "string"
          },
          "nextUrl": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "AuthenticatePasswordInputModel": {
        "required": [
          "password",
          "username"
        ],
        "type": "object",
        "properties": {
          "username": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "staySignedIn": {
            "type": "boolean"
          },
          "nextUrl": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "AuthenticateInputModel": {
        "required": [
          "oneTimeCode",
          "username"
        ],
        "type": "object",
        "properties": {
          "username": {
            "type": "string"
          },
          "oneTimeCode": {
            "pattern": "^[0-9]{6}$",
            "type": "string"
          },
          "staySignedIn": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "AuthenticateLongCodeInputModel": {
        "required": [
          "longCode"
        ],
        "type": "object",
        "properties": {
          "longCode": {
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "SendPasswordResetMessageInputModel": {
        "required": [
          "username"
        ],
        "type": "object",
        "properties": {
          "applicationId": {
            "type": "string",
            "nullable": true
          },
          "username": {
            "type": "string"
          },
          "nextUrl": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "UserInfo": {
        "type": "object",
        "properties": {
          "isAuthenticated": {
            "type": "boolean"
          },
          "username": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "AppInfo": {
        "type": "object",
        "properties": {
          "minimumPasswordStrength": {
            "type": "integer",
            "format": "int32"
          },
          "minimumPasswordLength": {
            "type": "integer",
            "format": "int32"
          },
          "user": {
            "$ref": "#/components/schemas/UserInfo"
          },
          "permissions": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
