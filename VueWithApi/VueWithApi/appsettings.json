{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=PasswordlessDemo;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "App": {
    "__FrontEnd__":  "valid options are VueJS or React" ,
    "FrontEnd": "React"
  },
  "IdProvider": {
    "DefaultSessionLengthMinutes": 60,
    "MaxSessionLengthMinutes": 43200,
    "ChangeSecuritySettingsTimeWindowMinutes": 5,
    "CancelEmailChangeTimeWindowHours": 72,
    "MinimumPasswordLength": 8,
    "MinimumPasswordStrengthInBits": 30,
    "RememberUsernames": true,
    "CookieSameSiteMode": "None",
    "Urls": {
      "CustomApiBase": "/api",
      "DefaultRedirect": "/",
      "MyAccount": "/myaccount",
      "ForgotPassword": "/forgotpassword",
      "Register": "/register",
      "SetPassword": "/setpassword",
      "SignIn": "/signin",
      "SignInLink": "/signin/{long_code}",
      "SignOut": "/signout",
      "Error": "/error"
    }
  }
}
